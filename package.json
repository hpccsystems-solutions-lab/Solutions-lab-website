{
  "name": "hpccsystems-solutions-lab",
  "description": "Documentation website for hpccsystems-solutions-lab.",
  "version": "1.0.0",
  "private": true,
  "author": "Jerry Jacob",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "commit": "git cz",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "dev": "gatsby develop",
    "format": "npm run format:src && npm run format:docs",
    "format:src": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:docs": "prettier --write \"docs/**/*.md\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "start": "serve -s public",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
